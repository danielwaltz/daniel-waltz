    "@iconify-json/simple-icons": "^1.2.32",
    "@iconify/utils": "^2.3.0",
    "@nuxt/content": "^3.4.0",
    "@nuxt/devtools": "^2.4.0",
    "@nuxt/eslint": "^1.3.0",
    "@nuxt/fonts": "^0.11.1",
    "@nuxt/image": "^1.10.0",
    "@types/node": "~22.14.1",
    "@unocss/eslint-plugin": "^66.0.0",
    "@unocss/nuxt": "^66.0.0",
    "@unocss/reset": "^66.0.0",
    "@vueuse/nuxt": "^13.1.0",
    "better-sqlite3": "^11.9.1",
    "eslint": "^9.25.0",
