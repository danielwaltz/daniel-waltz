    "@typescript-eslint/parser": "5.29.0",
    "@vue/eslint-config-prettier": "7.0.0",
    "eslint": "8.18.0",
    "eslint-plugin-vue": "9.1.1",
    "npm-run-all": "^4.1.5",
    "nuxt": "3.0.0-rc.4",
    "prettier": "2.7.1",
    "typescript": "4.7.4",
    "vue-tsc": "0.38.1"
  }
}
