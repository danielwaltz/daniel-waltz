    "@iconify/utils": "^2.1.12",
    "@nuxt/devtools": "latest",
    "@nuxt/eslint-config": "^0.2.0",
    "@rushstack/eslint-patch": "^1.6.0",
    "@types/node": "~20.10.4",
    "@unocss/eslint-config": "^0.58.0",
    "@unocss/nuxt": "^0.58.0",
    "@vue/eslint-config-prettier": "^8.0.0",
    "eslint": "^8.55.0",
    "npm-run-all": "^4.1.5",
    "nuxt": "^3.8.2",
    "prettier": "^3.1.0",
    "typescript": "^5.3.3",
