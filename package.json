    "@types/node": "~18.17.19",
    "@unocss/eslint-config": "^0.57.1",
    "@unocss/nuxt": "^0.57.1",
    "@vue/eslint-config-prettier": "^8.0.0",
    "eslint": "^8.52.0",
    "npm-run-all": "^4.1.5",
    "nuxt": "^3.8.0",
    "prettier": "^3.0.3",
    "typescript": "^5.2.2",
    "vue-tsc": "^1.8.22"
