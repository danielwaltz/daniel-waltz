{
  "name": "daniel-waltz",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "nuxt dev",
    "build": "nuxt build",
    "preview": "nuxt preview",
    "generate": "nuxt generate",
    "lint": "eslint . --ext .js,.jsx,.cjs,.mjs,.ts,.tsx,.cts,.mts,.vue --no-fix --ignore-path .gitignore --max-warnings=0",
    "type-check": "vue-tsc --noEmit",
    "test": "run-p lint type-check",
    "deploy": "run-s build test generate"
  },
  "devDependencies": {
    "@nuxtjs/eslint-config-typescript": "^11.0.0",
    "@rushstack/eslint-patch": "^1.2.0",
    "@typescript-eslint/eslint-plugin": "^5.43.0",
    "@typescript-eslint/parser": "^5.43.0",
    "@vue/eslint-config-prettier": "^7.0.0",
    "eslint": "^8.27.0",
    "eslint-plugin-vue": "^9.7.0",
    "npm-run-all": "^4.1.5",
    "nuxt": "^3.0.0-rc.14",
    "prettier": "^2.7.1",
    "typescript": "^4.9.3",
    "vue-tsc": "^1.0.9"
  }
}
