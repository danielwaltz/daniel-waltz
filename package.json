    "@typescript-eslint/eslint-plugin": "^5.60.0",
    "@typescript-eslint/parser": "^5.60.0",
    "@unocss/eslint-config": "^0.53.3",
    "@unocss/nuxt": "^0.53.3",
    "@vue/eslint-config-prettier": "^7.1.0",
    "eslint": "^8.43.0",
    "eslint-plugin-vue": "^9.15.1",
    "npm-run-all": "^4.1.5",
    "nuxt": "^3.6.0",
    "prettier": "^2.8.8",
