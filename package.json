{
  "name": "daniel-waltz",
  "private": true,
  "packageManager": "pnpm@10.8.1",
  "type": "module",
  "scripts": {
    "build": "nuxt build",
    "dev": "nuxt dev",
    "generate": "nuxt generate",
    "generate-ico": "pnpx icon-gen@latest -i ./public/favicon.svg -o ./public --ico --ico-name favicon --ico-sizes 16,32,48",
    "generate-png": "pnpx svg2png@latest ./public/favicon.svg -o ./public/favicon.png -w 512 -h 512",
    "lint": "eslint --cache --cache-location ./node_modules/.cache/.eslintcache --max-warnings 0",
    "postinstall": "nuxt prepare",
    "preview": "nuxt preview",
    "test": "run-p -clns lint type-check",
    "type-check": "nuxt typecheck"
  },
  "devDependencies": {
    "@csstools/postcss-oklab-function": "^4.0.8",
    "@danielwaltz/eslint-config": "^2.2.1",
